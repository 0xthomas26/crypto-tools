{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["prettier", "next", "next/core-web-vitals"],
    "parserOptions": {
        "ecmaFeatures": {
            "tsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "semi": ["error", "always"]
    }
}
